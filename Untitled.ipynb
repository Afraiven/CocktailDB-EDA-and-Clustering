       "        id                        name             category                glass                                                                                     tags                                                                                                                                                                                                                                                                                                   instructions                                                                       imageUrl  alcoholic                      createdAt                      updatedAt                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        